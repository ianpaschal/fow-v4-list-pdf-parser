{
  "name": "fow-v4-list-parser",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.js --external:pdfjs-dist/legacy/build/pdf.mjs --external:fs --external:path",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "A TypeScript project for parsing FOW v4 lists",
  "dependencies": {
    "@ianpaschal/combat-command-static-data": "^3.21.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^2.1.4",
    "pdfjs-dist": "^5.4.149"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.3.0",
    "@stylistic/eslint-plugin-ts": "^2.3.0",
    "@stylistic/stylelint-config": "^2.0.0",
    "@stylistic/stylelint-plugin": "^3.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "esbuild": "^0.25.10",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "typescript": "^5.0.0"
  }
}
